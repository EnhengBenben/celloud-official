<div class="content">
    <table class="contentTable">
        <tr class="focus">
            <td style="font-size:20px;"><br/>结果解读：</td>
        </tr>
        <tr class="focus hr">
            <td><hr/></td>
        </tr>
        <tr>
            <td>胚胎编号:<span><input name="baseInfo['explainEmbryoNumber']" value="$!{pgs.baseInfo.explainEmbryoNumber}" type="text" class="tdInput"></span></td>
        </tr>
        <tr>
            <td><div style="float:left">检测结果:</div><div style="float:left;width:55%;word-wrap: break-word;word-break: break-all;" id="explainResult"><textarea style="width:100%" rows="3" name="baseInfo['explainResult']">#if(${pgs.baseInfo.explainResult})$!{pgs.baseInfo.explainResult}#else#end</textarea></div></td>
        </tr>
        <tr>
            <td><div style="float:left">遗传咨询:</div><div style="float:left;width:55%;word-wrap: break-word;word-break: break-all;" id="explainGeneticCounseling"><textarea style="width:100%" rows="3" name="baseInfo['explainGeneticCounseling']">#if(${pgs.baseInfo.explainGeneticCounseling})$!{pgs.baseInfo.explainGeneticCounseling}#else#end</textarea></div></td>
        </tr>
    </table>
    #if($flag==0 || $flag==1)
    	<img src="${uploadPath}/${pgs.userId}/${pgs.appId}/${pgs.dataKey}/${pgs.miniPng}" style="width:100%;">
    	<img src="${uploadPath}/${pgs.userId}/${pgs.appId}/${pgs.dataKey}/${pgs.splitPng}" style="width:100%;">
    #elseif($flag==2 || $flag==3)
    	#if(${pgs.reportMiniPng})
    		<img src="${uploadPath}/${pgs.userId}/${pgs.appId}/${pgs.dataKey}/${pgs.reportMiniPng}" style="width:100%;">
    	#end
    	#if(${pgs.reportSplitPng})
    		<img src="${uploadPath}/${pgs.userId}/${pgs.appId}/${pgs.dataKey}/${pgs.reportSplitPng}" style="width:100%;">
    	#end
    #end
</div>