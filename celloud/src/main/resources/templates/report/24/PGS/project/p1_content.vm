<div class="content">
    <input type="hidden" name="id" value="#if($pgs)$!{pgs.id}#else${pgsList.get(0).id}#end" id="id" />
    <input type="hidden" name="projectId" value="${pgsList.get(0).projectId}" />
    <input type="hidden" name="exists" value="${exists}" id="exists" />
    <table class="contentTable">
        <tr class="focus">
            <td colspan="4" style="font-size:20px;"><br/>基本信息：</td>
        </tr>
        <tr class="focus hr">
            <td colspan="4"><hr/></td>
        </tr>
        <tr>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">女方姓名:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['womanName']">$!{pgs.projectInfo.womanName}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">年&nbsp;&nbsp;&nbsp;&nbsp;龄:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['womanAge']">$!{pgs.projectInfo.womanAge}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">男方姓名:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['manName']">$!{pgs.projectInfo.manName}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">年&nbsp;&nbsp;&nbsp;&nbsp;龄:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['manAge']">$!{pgs.projectInfo.manAge}</textarea></div></td>
        </tr>
        <tr>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">病历号:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['recordNumber']">$!{pgs.projectInfo.recordNumber}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">实验编号:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['experimentNumber']">$!{pgs.projectInfo.experimentNumber}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">样本类型:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['sampleType']">$!{pgs.projectInfo.sampleType}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">胚胎数量:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['embryoCount']">$!{pgs.projectInfo.embryoCount}</textarea></div></td>
        </tr>
        <tr>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">活检时期:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['biopsyPeriod']">$!{pgs.projectInfo.biopsyPeriod}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">样本状态:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['embryoState']">$!{pgs.projectInfo.embryoState}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">接收时间:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['receiveDate']">$!{pgs.projectInfo.receiveDate}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">检测时间:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['detectionDate']">$!{pgs.projectInfo.detectionDate}</textarea></div></td>
        </tr>              
        <tr>                           
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">送检单位:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['inspectionUnit']">$!{pgs.projectInfo.inspectionUnit}</textarea></div></td>
            <td></td>                  
            <td style="vertical-align: top;width: 25%;"><div style="float:left;width:35%;">送检人:</div><div class="baseTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:63%;"><textarea style="width:100%" rows="1" name="projectInfo['forPeople']">$!{pgs.projectInfo.forPeople}</textarea></div></td>
            <td></td>
        </tr>
        <tr>
            <td style="vertical-align: top;width: 100%;" colspan="2"><div style="float:left;width:17%;">临床诊断:</div><div style="float:left;width:10%;">女方:</div><div class="womanBaseClinicalTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:72%;"><textarea style="width:100%" rows="1" name="projectInfo['womanClinical']">$!{pgs.projectInfo.womanClinical}</textarea></div></td>
            <td style="vertical-align: top;width: 25%;" colspan="2"><div style="float:left;width:10%;">男方:</div><div class="manBaseClinicalTextarea" style="float:left;word-wrap: break-word;word-break: break-all;width:89%;"><textarea style="width:100%" rows="1" name="projectInfo['manClinical']">$!{pgs.projectInfo.manClinical}</textarea></div></td>
        </tr>
        <tr class="focus">
            <td colspan="4" style="font-size:20px;"><br/>检测内容：</td>
        </tr>
        <tr class="focus hr">
            <td colspan="4"><hr/></td>
        </tr>
        <tr class="focus">
            <td colspan="4">检测项目：胚胎染色体异常检测</td>
        </tr>
        <tr class="focus">
            <td colspan="4">检测方法：高通量测序--Ion PGM™测序平台</td>
        </tr>
        <tr class="focus">
            <td colspan="4">检测类型：胚胎染色体非整倍体及大于4Mb的染色体缺失和重复</td>
        </tr>
        <tr class="focus">
            <td colspan="4">检测结果：参考基因组为GRCh37/hg19　　详细结果见“结果解读”</td>
        </tr>
        <tr class="focus hr">
            <td colspan="4"><hr/></td>
        </tr>
        <tr style="background-color:#e0e0e0;">
            <td class="tdCenter">胚胎编号</td>
            <td class="tdCenter">检测结果</td>
            <td class="tdCenter">胚胎评级</td>
            <td class="tdCenter">临床建议</td>
        </tr>
    </table>
    <table class="contentTable">
    	#if($pgsList.size() > 1)
	        #foreach($p in $pgsList)
	        	#set($index=$velocityCount - 1)
	        	#if($p.noEnoughReads == 'false')
			        <tr style="height:60px;">
			            <td class="tdCenter" style="width:25%;"><div class="embryoNumber" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['embryoNumber']">#if(${pgs.projectInfo.embryoNumber.get($index)})$!{pgs.projectInfo.embryoNumber.get($index)}#else$!{p.anotherName}#end</textarea></div></td>
			            <td class="tdCenter" style="width:25%;"><div class="result" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['result']">#if(${pgs.projectInfo.result.get($index)})$!{pgs.projectInfo.result.get($index)}#else$!{p.report}#end</textarea></div></td>
			            <td class="tdCenter" style="width:25%;"><div class="embryoRating" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['embryoRating']">$!{pgs.projectInfo.embryoRating.get($index)}</textarea></div></td>
		            	<td class="tdCenter" style="width:25%;"><div class="advice" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['advice']">$!{pgs.projectInfo.advice.get($index)}</textarea></div></td>
			        </tr>
			    #else
			    	<tr style="height:60px;">
			            <td class="tdCenter" style="width:25%;"><div class="embryoNumber" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['embryoNumber']">#if(${pgs.projectInfo.embryoNumber.get($index)})$!{pgs.projectInfo.embryoNumber.get($index)}#else$!{p.anotherName}#end</textarea></div></td>
			            <td class="tdCenter" style="width:25%;"><div class="result" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['result']">#if(${pgs.projectInfo.result.get($index)})$!{pgs.projectInfo.result.get($index)}#else测序量不足，无法分析，建议重测。#end</textarea></div></td>
			            <td class="tdCenter" style="width:25%;"><div class="embryoRating" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['embryoRating']">$!{pgs.projectInfo.embryoRating.get($index)}</textarea></div></td>
		            	<td class="tdCenter" style="width:25%;"><div class="advice" style="word-break: break-all;width:99%;"><textarea style="width:98%;max-height:50px;height:50px;font-size:14px;" name="projectInfo['advice']">$!{pgs.projectInfo.advice.get($index)}</textarea></div></td>
			        </tr>
			    #end
	        #end
        #else
        	#if($pgsList.get(0).noEnoughReads == 'false')
        		<tr style="height:60px;">
		            <td class="tdCenter" style="width:25%;"><div class="embryoNumber" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['embryoNumber']">#if(${pgs.projectInfo.embryoNumber})$!{pgs.projectInfo.embryoNumber}#else$!{pgsList.get(0).anotherName}#end</textarea></div></td>
		            <td class="tdCenter" style="width:25%;"><div class="result" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['result']">#if(${pgs.projectInfo.result})$!{pgs.projectInfo.result}#else$!{pgsList.get(0).report}#end</textarea></div></td>
		            <td class="tdCenter" style="width:25%;"><div class="embryoRating" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['embryoRating']">$!{pgs.projectInfo.embryoRating}</textarea></div></td>
	            	<td class="tdCenter" style="width:25%;"><div class="advice" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['advice']">$!{pgs.projectInfo.advice}</textarea></div></td>
		        </tr>
        	#else
        		<tr style="height:60px;">
		            <td class="tdCenter" style="width:25%;"><div class="embryoNumber" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['embryoNumber']">#if(${pgs.projectInfo.embryoNumber})$!{pgs.projectInfo.embryoNumber}#else$!{pgsList.get(0).anotherName}#end</textarea></div></td>
		            <td class="tdCenter" style="width:25%;"><div class="result" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['result']">#if(${pgs.projectInfo.result})$!{pgs.projectInfo.result}#else测序量不足，无法分析，建议重测。#end</textarea></div></td>
		            <td class="tdCenter" style="width:25%;"><div class="embryoRating" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['embryoRating']">$!{pgs.projectInfo.embryoRating}</textarea></div></td>
	            	<td class="tdCenter" style="width:25%;"><div class="advice" style="word-break: break-all;width:99%;"><textarea style="width:98%;height:50px;font-size:14px;" name="projectInfo['advice']">$!{pgs.projectInfo.advice}</textarea></div></td>
		        </tr>
        	#end
        #end
        <tr class="focus hr">
            <td colspan="4"><br/><hr/></td>
        </tr>
    </table>
</div>