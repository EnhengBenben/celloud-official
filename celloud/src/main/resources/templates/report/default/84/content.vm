<form id="print-form">
    <input type="hidden" name="id" value="${egfr.id}" id="egfrId" />
    <input type="hidden" value="${egfr.projectId}" id="_projectId">
    <div style="display: none;" id="_userId">${egfr.userId}</div>
    <div style="display: none;" id="_appId">${egfr.appId}</div>
    <div style="display: none;" id="_fileId">${report.fileId}</div>
    <div class="wrapper">
        <ul class="info mt5">
            <li>姓名：<span><input name="baseInfo['name']" value="$!{egfr.baseInfo.name}" type="text"></span></li>
            <li>ID号：<span><input name="baseInfo['id']" value="$!{egfr.baseInfo.id}" type="text"></span></li>
            <li>样本编号：<span><input name="baseInfo['sampleNumber']" value="$!{egfr.baseInfo.sampleNumber}" type="text"></span></li>
            <li>样本种类：<span><input name="baseInfo['sampleType']" value="$!{egfr.baseInfo.sampleType}" type="text"></span></li>
            <li>性别：<span id="_sex"><input type="radio" name="baseInfo['sex']" value="男" #if(${egfr.baseInfo.sex}!='女')checked="checked"#end onclick="radioClick(0)" id="men">男<input type="radio" name="baseInfo['sex']" #if(${egfr.baseInfo.sex}=='女')checked="checked"#end value="女" onclick="radioClick(1)" id="women">女</span></li>
            <li>科室：<span><input name="baseInfo['dept']" value="$!{egfr.baseInfo.dept}" type="text"></span></li>
            <li>送检日期：<span><input name="baseInfo['submissionDate']" value="$!{egfr.baseInfo.submissionDate}" type="text"></span></li>
            <li>送检医生：<span><input name="baseInfo['doctor']" value="$!{egfr.baseInfo.doctor}" type="text"></span></li>
            <li>年龄：<span><input name="baseInfo['age']" value="$!{egfr.baseInfo.age}" type="text"></span>岁</li>
            <li>床号：<span><input name="baseInfo['bedNo']" value="$!{egfr.baseInfo.bedNo}" type="text"></span></li>
            <li>住院号：<span><input name="baseInfo['inpatientNumber']" value="$!{egfr.baseInfo.inpatientNumber}" type="text"></span></li>
        </ul>
    </div>
    <hr class="hr-bold mt5" />
    <div class="wrapper" style="min-height:800px;" id="mainDIv">
        <h2 class="mt20">已知位点：</h2>
        <p>
            #if(${egfr.position})
                ${egfr.position}
            #else
                未检测到突变
            #end
        </p>
        #if(${egfr.knowMutationBig.size()}>0)
            #foreach(${kmb} in ${egfr.knowMutationBig})
                <p>
                    <br/>
                    <img src="${uploadPath}${egfr.userId}/${egfr.appId}/${egfr.dataKey}/SVG/${kmb}" style="width: 750px;height: 112.5px;">
                </p>
            #end
        #end
        #if(${egfr.conclusion})
            <h2 class="mt20">结论：</h2>
            <p>${egfr.conclusion}</p>
        #end
        #if(${egfr.seq})
            <h2 class="mt20">原始序列：</h2>
            <p style="word-break: break-all;">${egfr.seq}</p>
        #end
        <div class="w3cbbs"></div>
        <h2 class="mt20">原始峰图：</h2>
        <p style="width:750px;">
            #if(${egfr.original.size()}>0)
                #foreach(${original} in ${egfr.original})
                    <img src="${uploadPath}${egfr.userId}/${egfr.appId}/${egfr.dataKey}/SVG/${original}" style="width: 100%;">
                    <br/><br/>
                #end
            #else
                样本异常，无法检测
            #end
        </p>
    </div>
