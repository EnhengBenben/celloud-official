          #end
          </td>
          <td style="width:49%">
          #if($bsi.pngPath.top6png)
            <img src="$!uploadPath/$!bsi.userId/$!bsi.appId/$!bsi.dataKey$!bsi.pngPath.top6png" style="width:90%;">
          #end
