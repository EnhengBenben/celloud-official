#parse("templates/report/33/118/layout.vm")
<link href="${base}/css/print_bsi.css?version=3.4.13" rel="stylesheet">
<style>
.tests-content {
    border-top: none;
}
.width-35 {
	float: left;
    width: 35%;
}
.width-30 {
	float: left;
    width: 30%;
}
.width-20 {
	float: left;
    width: 20%;
}
.width-15 {
	float: left;
    width: 15%;
}
.clearBoth{
	clear: both;
}

.base-info{
	padding-bottom: 0;
}
.base-info ul>li{
	margin-bottom: 0;
}
.tests-content{
	padding-top:0;
}

</style>
<div class="content yanda">
  <div class="print-button">
    <a id="print-report" class="btn" title="打印" href="javascript:void(0)"><i class="fa fa-print"></i></a>
    <a id="reset-info" class="btn" title="清空" href="javascript:void(0)">
    <svg width="16" height="16" viewBox="0 0 200 200" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <g class="transform-group">
    <g transform="scale(0.1953125, 0.1953125)">
